{
	"name": "app-dev",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "app-dev",
	"workspaceFolder": "/root/workspace",
	"extensions": [
		//java
		"vscjava.vscode-java-pack",
		// git extensions that are highly recommended
		"eamodio.gitlens",
		"mhutchie.git-graph",
		// nice file icons from vscode icons team
		"vscode-icons-team.vscode-icons"
	],
	"forwardPorts": [ 8080 ],
}